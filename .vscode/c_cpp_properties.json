{
    "configurations": [
        {
            "name": "LINUX",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/bin"
              //  "${env:RISCV_TOOLCHAIN_PATH}/RISC-V_Embedded_GCC/riscv-none-embed",
              //  "${env:RISCV_TOOLCHAIN_PATH}/RISC-V_Embedded_GCC/riscv-none-embed/include"
